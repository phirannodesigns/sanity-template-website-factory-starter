{
  "name": "gatsby-starter-phiranno-designs",
  "private": true,
  "description": "Gatsby Starter for Phiranno Designs Websites",
  "version": "0.1.0",
  "author": "Luke Bennett <l.bennett@phirannodesigns.com.au>",
  "license": "0BSD",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build:serve": "npm run build && npm run serve",
    "clean": "gatsby clean",
    "develop": "gatsby develop --host=0.0.0.0",
    "dev": "npm run develop",
    "format": "prettier . --write --ignore-unknown",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint": "eslint --color --ext .js,.json,.jsx,.ts,.tsx src",
    "serve": "gatsby serve",
    "start": "npm run develop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@headlessui/react": "^0.3.2",
    "@heroicons/react": "^1.0.0",
    "@reach/router": "^1.3.4",
    "@sanity/block-content-to-react": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "gatsby": "^3.2.1",
    "gatsby-plugin-image": "^1.2.1",
    "gatsby-source-sanity": "^7.0.2",
    "intersection-observer": "^0.12.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.0.5",
    "react-icons": "^4.2.0",
    "react-intersection-observer": "^8.31.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "autoprefixer": "^10.2.5",
    "eslint-config-adjunct": "^4.11.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-auto": "^0.7.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-markdown": "^2.0.1",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^3.1.4",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.6.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^29.0.0",
    "gatsby-plugin-google-gtag": "^3.2.0",
    "gatsby-plugin-manifest": "^3.2.0",
    "gatsby-plugin-postcss": "^4.2.0",
    "gatsby-plugin-react-helmet": "^4.2.0",
    "gatsby-plugin-sharp": "^3.2.1",
    "gatsby-plugin-sitemap": "^3.2.0",
    "gatsby-plugin-typescript": "^3.2.0",
    "gatsby-source-filesystem": "^3.2.0",
    "gatsby-transformer-sharp": "^3.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.10",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.1",
    "typescript": "^4.2.4"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "auto"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/no-floating-promises": 0,
      "@typescript-eslint/no-unsafe-assignment": 0,
      "@typescript-eslint/no-unsafe-call": 0,
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/no-use-before-define": 0,
      "arrow-body-style": 0,
      "eslint-comments/disable-enable-pair": 0,
      "import/prefer-default-export": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "jsx-a11y/no-noninteractive-tabindex": 0,
      "no-secrets/no-secrets": 0,
      "no-underscore-dangle": 0,
      "no-unused-vars": 1,
      "react/jsx-filename-extension": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/no-array-index-key": 1,
      "react/no-danger": 1,
      "react/no-unescaped-entities": 0,
      "react/no-unused-prop-types": 1,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 0,
      "react/require-default-props": 0,
      "scanjs-rules/assign_to_src": 0,
      "scanjs-rules/identifier_localStorage": 0,
      "scanjs-rules/property_localStorage": 0,
      "sonarjs/no-duplicate-string": 1,
      "unicorn/no-array-reduce": 0,
      "unicorn/no-new-array": 0,
      "unicorn/no-useless-undefined": 0,
      "unicorn/prefer-spread": 0
    }
  }
}
